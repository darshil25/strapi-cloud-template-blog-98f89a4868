    },
    "course_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-category.course-category",
      "mappedBy": "courses"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "courses"
    },
    "chapters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chapter.chapter",
      "mappedBy": "courses"
    }
